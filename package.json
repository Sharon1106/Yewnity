  },
  "homepage": "https://github.com/Sharon1106/Yewnity#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.25.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "getstream": "^7.2.6",
